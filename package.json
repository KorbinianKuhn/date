{
  "name": "@korbiniankuhn/datetime",
  "version": "1.0.3",
  "description": "High performance date manipulations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "shx rm -rf dist",
    "test": "jest test/**",
    "test:coverage": "jest --coverage",
    "lint": "tslint src/**/*.ts && eslint test/**/*.js",
    "lint:fix": "tslint src/**/*.ts --fix && eslint test/**/*.js --fix",
    "precommit": "npm audit && lint-staged && npm build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KorbinianKuhn/datetime.git"
  },
  "keywords": [
    "date",
    "time",
    "utilities",
    "manipulation",
    "operations",
    "high performance",
    "typescript"
  ],
  "author": "Korbinian Kuhn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KorbinianKuhn/datetime/issues"
  },
  "homepage": "https://github.com/KorbinianKuhn/datetime#readme",
  "devDependencies": {
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "moment": "^2.22.2",
    "prettier": "^1.15.2",
    "shx": "^0.3.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint": "^5.9.0",
    "typescript": "^3.1.6"
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
